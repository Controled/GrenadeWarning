local function clamp(val, min, max)
    if val > max then
        return max
    elseif val < min then
        return min
    else
        return val
    end
end
local alpha_smoke = 255
local font = draw.CreateFont("segoe ui", 30, 600)
local function smoke(builder)
    local ent = builder:GetEntity()
    local lp = entities.GetLocalPlayer()
    local x, y, x1, x2 = builder:GetRect()

    local tick_current = globals.TickCount()
    local seconds_per_tick = globals.TickInterval()

    local lp_abs = lp:GetAbsOrigin()
    local abs_origin = ent:GetAbsOrigin()

    local fade = ((1.0 / 0.15) * globals.FrameTime()) * 50

    local dist = vector.Distance({abs_origin.x, abs_origin.y, abs_origin.z}, {lp_abs.x, lp_abs.y, lp_abs.z})

    if ent:GetName() == "CSmokeGrenadeProjectile" then
        local ticks = ent:GetProp("m_nSmokeEffectTickBegin")
        local time_since_explosion = seconds_per_tick * (tick_current - ticks)

        alpha_smoke = dist > 1500 and clamp(alpha_smoke - fade, 0, 200) or clamp(alpha_smoke + fade, 0, 200)

        if alpha_smoke ~= 0 then
            for i = 1, 3, 0.2 do
                local a = i / 10 * alpha_smoke
                draw.FilledCircle(x1 - 11.8, x2 - 8 - (2 + dist / 800), (20 - dist / 150) + i, draw.Color(16, 16, 16, a))
            end

            builder:Color(255, 255, 255, alpha_smoke * 1.275)
            builder:AddIconTop(smoke_svg)

            if time_since_explosion > 0 and time_since_explosion < 18 + 1 then
                local time = string.format("%.1f", 18 - time_since_explosion)
                for i = 1, 3, 0.2 do
                    local a = i / 10 * alpha_smoke
                    renderer.circle_outline(
                        x1 - 11.8,
                        x2 - 8 - (2 + dist / 800),
                        255,
                        255,
                        255,
                        a,
                        (20 - dist / 150) + i,
                        0,
                        time * (0.055 + i * 0.0005),
                        (3 - dist / 300) + (i * 2)
                    )
                end
            else
                for i = 1, 3, 0.2 do
                    local a = i / 10 * alpha_smoke
                    renderer.circle_outline(
                        x1 - 11.8,
                        x2 - 8 - (2 + dist / 800),
                        255,
                        255,
                        255,
                        a,
                        (20 - dist / 150) + i,
                        0,
                        1,
                        (3 - dist / 300) + (i * 2)
                    )
                end
            end
        end
    elseif ent:GetName() == "CMolotovProjectile" then
        if dist < 200 then
            for i = 1, 3, 0.2 do
                local a = i / 10 * 200
                draw.FilledCircle(x1 - 11.8, x2 - 8 - (1 + dist / 1000), (20 - dist / 200) + i, draw.Color(255, 0, 0, a))
            end

            draw.Text(x1 - 15, x2 - 15 - (2 + dist / 800), "!", draw.Color(255, 255, 255, 255), draw.SetFont(font))
        else
            for i = 1, 3, 0.2 do
                local a = i / 10 * 200
                draw.FilledCircle(x1 - 11.8, x2 - 8 - (1 + dist / 1000), (20 - dist / 200) + i, draw.Color(4, 4, 4, a))
            end

            builder:Color(255, 255, 255, 255)
            builder:AddIconTop(molotov_svg)
        end
    elseif ent:GetName() == "CBaseCSGrenadeProjectile" then
        if dist < 280 then
            for i = 1, 3, 0.2 do
                local a = i / 10 * 200
                draw.FilledCircle(x1 - 11.8, x2 - 8 - (1 + dist / 1000), (20 - dist / 200) + i, draw.Color(255, 0, 0, a))
            end

            draw.Text(x1 - 15, x2 - 15 - (2 + dist / 800), "!", draw.Color(255, 255, 255, 255), draw.SetFont(font))
        else
            for i = 1, 3, 0.2 do
                local a = i / 10 * 200
                draw.FilledCircle(x1 - 11.8, x2 - 8 - (1 + dist / 1000), (20 - dist / 200) + i, draw.Color(4, 4, 4, a))
            end

            builder:Color(255, 255, 255, 255)
            builder:AddIconTop(explosive_svg)
        end
    end
end
callbacks.Register("DrawESP", smoke)

